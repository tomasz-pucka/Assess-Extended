<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UTA"
        xmlns:ViewModels="clr-namespace:UTA.ViewModels" x:Class="UTA.Views.MainView"
        mc:Ignorable="d"
        Title="MainView" Height="450" Width="800">
    <Window.DataContext>
        <ViewModels:VariantsTestList/>
    </Window.DataContext>
    <Grid>
        <ListView HorizontalAlignment="Left" Height="101" Margin="330,126,0,0" VerticalAlignment="Top" Width="141" RenderTransformOrigin="-0.638,-0.062" ItemsSource="{Binding VList}">
         <ListView.ItemTemplate>
             <DataTemplate>
                 <TextBlock Text="{Binding Name}"/>
             </DataTemplate>
         </ListView.ItemTemplate>
        </ListView>
    </Grid>
</Window>
